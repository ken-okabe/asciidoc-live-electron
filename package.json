{
  "name": "asciidoc-live-electron",
  "displayName": "asciidoc-live-electron",
  "description": "AsciiDoc Live Electron",
  "version": "0.0.1",
  "publisher": "kenokabe",
  "engines": {
    "vscode": "^1.28.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.asciidoc-live-electron-connect"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.asciidoc-live-electron-connect",
        "title": "AsciiDoc Live Electron: Activate & Ping-Test to Viewer"
      },
      {
        "command": "extension.asciidoc-live-electron-saveHTML",
        "title": "AsciiDoc Live Electron: Save HTML of the doc on Viewer"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.10.38",
    "typescript": "^2.6.1",
    "vscode": "^1.1.21"
  },
  "dependencies": {
    "json-socket-international": "*"
  }
}
