{
    "name": "asciidoc-live-electron",
    "displayName": "asciidoc-live-electron",
    "description": "AsciiDoc Live Electron",
    "version": "0.0.1",
    "publisher": "kenokabe",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.asciidoc-live-electron-connect"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.asciidoc-live-electron-connect",
                "title": "AsciiDoc Live Electron: Connect to Viewer"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "@types/socket.io-client": "^1.4.32",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "socket.io-client": "^2.1.1"
    }
}